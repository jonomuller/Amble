platform :ios, '9.0'

target 'amble-ios' do
  use_frameworks!

  # Pods for amble-ios
  pod 'SwiftyJSON'
  pod 'Alamofire', '~> 4.4'
  pod 'Locksmith'
  pod 'ChameleonFramework/Swift', :git => 'https://github.com/ViccAlexander/Chameleon.git'

  target 'amble-iosTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Mockingjay'
  end

  target 'amble-iosUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
